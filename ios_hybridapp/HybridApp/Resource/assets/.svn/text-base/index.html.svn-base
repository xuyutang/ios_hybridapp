var count = 0;
//滚动条划到底部时触发调用(如实现延迟加载)
function onPageEnd(){
    if (count == 10)
    {
        App.prompt('没有更多了');
        return;
    }

    for(var i = 1;i <= 5; i++)
    {
        $('<a id="'+(10+count + i)+'" class="weui-cell weui-cell_access" href="javascript:App.openNew(base+\'/detail.html\')">'+
            '<div class="weui-cell__bd p-l-50">'+
                '<p class="f16 f333">2017-11-'+(10+count + i)+'工作日志</p>    '+
                '<p class="f12 f999 three-h p-t-6"><span class="f666">工作内容：</span>解决了一些难点存在问题</p>    '+
                '<p class="f12 f999 three-h p-t-6"><span class="f666">存在问题：</span>视频播放还存在个别难点需要解决</p>'+
            '</div>'+
        '</a>').appendTo("#content");
    }
    count += 5;
}


function onResume(){
    if (App.get("logName")){//读取变量
        App.prompt(App.get("logName"));
        //删除变量
        App.remove("logName");
    }
}

